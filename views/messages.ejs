<div id="messages">
<% Object.keys(locals.messages).forEach(function (type) { %>
  <% messages[type].forEach(function (message) { %>
    <div class='alert alert-info'>> <%=message-%></div>
  <% }) %>
<% }) %>
</div>

<div id="messages">
<% Object.keys(messages).forEach(function (type) { %>
  <ul class="<%= type %>">
  <% messages[type].forEach(function (message) { %>
    <li><%= message %></li>
  <% }) %>
  </ul>
<% }) %>
</div>
