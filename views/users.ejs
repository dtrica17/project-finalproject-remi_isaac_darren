<!DOCTYPE html>
<html>
<%- include('./head'); -%>
<body>
  <%- include('./navbar'); -%>
  <div class="container-fluid">
      <main class="p-3">
        <h1>Calendar: </h1>
        <!-- This is the calendar view it displays all events -->
        <%_ for (const e of allUsers) { -%>
          <p><%=e.name-%></p>
          <!-- this date format needs to be fixed -->
          <p class="ml-3"><%=e.date-%></p>
        <%_ } -%>
      </main>
  </div>
</body>
</html>
